@model guestACCESS.ClientWeb.ActionState
@{
    ViewBag.Title = "Trip Not Available";
    Layout = "~/Views/Shared/_LayoutCatalog.cshtml";
}

@section HeadContent {

    <style>
        .footer {
            position: absolute;
        }
    </style>

}

<div class="container">
    <!-- Main content -->
    <section class="content">
        <div class="error-page">
            <h2 class="headline text-green"><i class="fa fa-ban text-red"></i></h2>
            <div class="error-content">
                <h3>Trip Not Available!</h3>
                <p>
                    @Model.Message
                </p>
                <ul>
                    <li>Please contact our staff for help.</li>
                </ul>
            </div>
        </div>
    </section>
    <!-- /.content -->
</div>
<!-- /.container -->
